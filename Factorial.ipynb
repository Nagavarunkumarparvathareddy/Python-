   "source": [
    "print(\"\"\"Factorial of a Number\n",
    "==================================================================================================================================\"\"\")\n",
    "n = int(input())\n",
    "m = n\n",
    "for i in range(1,n):\n",
    "    factorial = n*(m-i)\n",
    "    n = factorial\n",
    "print(factorial)\n"
