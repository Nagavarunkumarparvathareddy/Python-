   ],
   "source": [
    "s = input('Enter Number or string').lower()\n",
    "l = len(s)\n",
    "left = 0\n",
    "right = l-1\n",
    "flag = True\n",
    "while left < right:\n",
    "    if s[left] != s[right]:\n",
    "        flag = False\n",
    "    left += 1\n",
    "    right -= 1\n",
    "if flag == True:\n",
    "    print('Palindrome')\n",
    "else:\n",
    "    print('Not a Palindrome')"
   ]
  },
